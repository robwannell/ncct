<body>
	
	
	<div class="headerbg">
		<div class="container logodiv">
			<% if current_user %><div class="row">
			<div class="col-md-offset-10 adminlogin"><%= link_to 'Admin Dashboard', admin_path, class: "badge adminbadge" %></div>
		</div><%end %>
		</div>
	
	</div>
	
	
	<div class="navbg">
	
  <div class="navbar" role="navigation">
    <div class="container">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
       
      </div>
      <div class="collapse navbar-collapse">
        <ul class="nav navbar-nav">
         <%= render '/layouts/nav.html.erb' %>
        </ul>
      </div><!-- /.nav-collapse -->
    </div><!-- /.container -->
  </div><!-- /.navbar -->

</div>
 
	 <div class="container">
		<div class="notices">
		<% if notice %>
		<h2 class="alert alert-success"><%= notice %></h2>
		<% end %>
		<% if alert %>
		<h2 class="alert alert-danger"><%= alert %></h2>
		<% end %>
		</div>
		</div>
	
	<div class="wrapper"> 
        
		
		


		                    
					
						
       
       
     
    <div class="pagecontainer">
		
		
    <div class="container page">

 <div class="row">
	 <div class="col-md-8">
 <%= yield %>
 	</div>
	
	
	
	<div class="col-md-4 rh">
		<%= yield :rh %>
	</div>
	
</div>
 <div class="row">
	 <div class="col-md-offset-7">
        <%= link_to '<span class="glyphicon-class glyphicon glyphicon-chevron-up"></span> Back to top'.html_safe, '#Top' %>
      </div>
  </div>
</div>
</div>
	</div>
  <footer>
	  <div class="container">
   <div class="footnav"><ul><%= render '/layouts/nav.html.erb' %></ul></div>
      <div class="facebook"><%= image_tag 'fb.png' %>  &nbsp;<%= link_to "NCCT on Facebook", 'http://www.facebook.com/NorthernCaliforniaConstructionTrainingInc', target: "_blank" %></div>
      <div class="copy">
       <p> &copy; NCCT, Inc. <%= Time.now.year %> |   <% if current_user %>
     
      <%= link_to "Logout", :logout, method: :post %> | <%= link_to 'Admin', admin_path %>
    <% else %>
     
      <%= link_to "Login", :login %>
    <% end %>
        
      </p>
	  
	  <div>
		  
		  
		  
		  
	     
